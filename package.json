{
  "name": "floorplan",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "NODE_OPTIONS='--loader ts-node/esm --experimental-specifier-resolution=node' TS_NODE_PROJECT=tsconfig.cucumber.json cucumber-js --config .cucumber.cjs",
    "test:headed": "HEADLESS=false NODE_OPTIONS='--loader ts-node/esm --experimental-specifier-resolution=node' TS_NODE_PROJECT=tsconfig.cucumber.json cucumber-js --config .cucumber.cjs",
    "test:ci": "NODE_OPTIONS='--loader ts-node/esm --experimental-specifier-resolution=node' TS_NODE_PROJECT=tsconfig.cucumber.json cucumber-js --config .cucumber.cjs"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^11.3.0",
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.50.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.4.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.10",
    "wait-on": "^8.0.1"
  }
}
